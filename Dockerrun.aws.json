{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitons": [
    {
      "name": "client",
      "image": "faissalelosman/multi-client-10-14",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "faissalelosman/multi-server-10-14",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "faissalelosman/multi-nginx-10-14",
      "essential": true,
      "portMappings": [{
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}